{
  "name": "ids-enterprise-ng-app",
  "version": "4.10.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod ids-enterprise-ng && ng build",
    "build:lib": "ng build --prod ids-enterprise-ng",
    "build:app": "ng build",
    "test:lib": "ng test ids-enterprise-ng",
    "test:app": "ng test",
    "pack:lib": "cd dist/ids-enterprise-ng && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "test": "ng test ids-enterprise-ng --browsers=PhantomJs --code-coverage",
    "lint": "ng lint && npm run mdlint",
    "mdlint": "npm run mdlint:docs && npm run mdlint:src",
    "mdlint:src": "npx markdownlint src",
    "mdlint:docs": "npx markdownlint docs/ && npx markdownlint README.md",
    "e2e": "ng e2e",
    "call-prepublishOnly": "cd ./publish && npm run prepublishOnly && cd ../",
    "update-enterprise": "node ./scripts/update-ids-enterprise.js",
    "version-bump:dev": "node ./scripts/version-append-date",
    "release:dev": "npm run update-enterprise && npm run version-bump:dev && npm publish publish/ --tag=dev",
    "release:beta": "cd publish/ && release-it minor --preRelease=beta",
    "release:rc": "cd publish/ && release-it --preRelease=rc",
    "release:final": "cd publish/ && release-it"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "ids-enterprise": "^4.9.0-beta.0",
    "jquery": "3.3.1",
    "lscache": "^1.2.0",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.1",
    "@angular-devkit/build-ng-packagr": "~0.7.1",
    "@angular/cli": "~6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "3.3.1",
    "@types/lscache": "^1.0.29",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "del": "^2.2.2",
    "gulp": "3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-filter": "^5.0.0",
    "gulp-replace": "^0.5.4",
    "gulp-string-replace": "^1.0.0",
    "htmlhint": "^0.9.13",
    "inquirer": "^6.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-bamboo-reporter": "^0.1.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "markdownlint-cli": "^0.10.0",
    "merge-stream": "^1.0.1",
    "ng-packagr": "^3.0.5",
    "protractor": "~5.3.0",
    "slash": "^2.0.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.32.1",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  }
}
